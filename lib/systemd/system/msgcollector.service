[Unit]
Description=msgcollector
Documentation=https://github.com/Whonix/msgcollector
Before=graphical.target
Before=getty.target

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/lib/msgcollector/msgdispatcher_init

[Install]
WantedBy=multi-user.target
